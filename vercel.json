{
    "builds": [
        {
            "src": "healthconnect_backend/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "healthconnect_backend/wsgi.py"
        }
    ]
}